                    </ul>&#13;
                    <div style="padding-left: 20px;">&#13;
                        <span id="ctl00_MainContent_lblLeftNumber">91</span>&#13;
                        <span> + </span>&#13;
                        <span id="ctl00_MainContent_lblRightNumber">32</span>&#13;
                        <span> = </span>&#13;
                        <input name="ctl00$MainContent$txtAnswer" type="text" id="ctl00_MainContent_txtAnswer" />&#13;
                        <input type="submit" name="ctl00$MainContent$btnAnswer" value="Continue" id="ctl00_MainContent_btnAnswer" />&#13;
                    </div>&#13;
                    <script type="text/javascript" language="javascript">&#13;
                        $('#ctl00_MainContent_txtAnswer').keydown(function (event) {&#13;
                            if (event.keyCode == 13) {&#13;
                                $('#ctl00_MainContent_btnAnswer').click();&#13;
                                return false;&#13;
                            }&#13;
                        }&#13;
                        );&#13;
                    </script>&#13;
                &#13;
            &#13;
