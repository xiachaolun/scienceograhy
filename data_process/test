            function ShowListByYear(year, type) {&#13;
                if (type == "publication") {&#13;
                    showPublicationsByYear(SiteRoot, publicationByYearDataUrlformat + year, "Publication in " + year, "divPaperList");&#13;
                    $("#hypListTitle").css("color", "#00bfe7");&#13;
                    $("#hypListTitle").attr("href", publicationByYearUrlformat + "&amp;year=" + year);&#13;
                }&#13;
                else if (type == "citation") {&#13;
                    showPublicationsByYear(SiteRoot, publicationByYearDataUrlformat + year + "&amp;citation=true", "Citation in " + year, "divPaperList");&#13;
                    $("#hypListTitle").css("color", "#ff711f");&#13;
                    $("#hypListTitle").attr("href", publicationByYearUrlformat.replace("&amp;searchtype=2", "&amp;searchtype=5") + "&amp;year=" + year);&#13;
