    //&lt;![CDATA[&#13;
    var redirectUrlctl00_MainContent_PaperList_OrderByFilter = 'http://academic.research.microsoft.com/Detail?entitytype=2&amp;searchtype=2&amp;id=594572';&#13;
    $(function () {&#13;
        //author&#13;
        var data = { 'inputId': '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drpList',&#13;
            "hiddenId": '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drp_hidden_focus',&#13;
            'textId': '#ctl00_MainContent_PaperList_OrderByFilter_drpList_spSelectedText',&#13;
            'tsId': '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drpListContent',&#13;
            'postAction': function () {   &#13;
                var value = $(DropDown.tsId + ' .selected').eq(0).find(':hidden').text();&#13;
                var url = window.location.href;             &#13;
                &#13;
                var regExp = /&amp;orderBy=\d+/ig;&#13;
                url = url.replace(regExp, "");                &#13;
                var origin_value = url.match(RegExp);&#13;
                if(value &gt; 0 &amp;&amp; typeof origin_value != "undefined" &amp;&amp; jQuery.trim(origin_value) == jQuery.trim(value)){&#13;
                    return;&#13;
                }&#13;
&#13;
                regExp = /&amp;start=\d+/ig;&#13;
                url = url.replace(regExp, "");&#13;
                regExp = /\?start=\d+/ig;&#13;
                url = url.replace(regExp, "?");&#13;
                regExp = /&amp;end=\d+/ig;&#13;
                url = url.replace(regExp, "");&#13;
                regExp = /\?end=\d+/ig;&#13;
                url = url.replace(regExp, "?");&#13;
&#13;
                if(redirectUrlctl00_MainContent_PaperList_OrderByFilter != '')&#13;
                {&#13;
                    //do not remain in the same page&#13;
                    url = redirectUrlctl00_MainContent_PaperList_OrderByFilter;&#13;
                }&#13;
&#13;
                if (value &gt; 0) {&#13;
                    url = url + "&amp;orderBy=" + value;&#13;
                    window.location = url;&#13;
                }&#13;
                else {&#13;
                    window.location = url;&#13;
                }&#13;
            }&#13;
        };&#13;
        DropDown.Init(data);&#13;
        var options = new Array();&#13;
&#13;
        options[0] = { "Text": "Year", "Value": "-1", "Selected": "true" };options[1] = { "Text": "Citation", "Value": "1" };options[2] = { "Text": "Rank", "Value": "2" };&#13;
&#13;
        DropDown.SetOptions(options);&#13;
        var startYear = GetStartYear();&#13;
        if (startYear &amp;&amp; startYear != "") {&#13;
            DropDown.SetDefaultValue(startYear);&#13;
        }&#13;
    })&#13;
&#13;
    function GetStartYear() {&#13;
        var lastYearValue = decodeURI(request("orderBy"));&#13;
        return lastYearValue;&#13;
    }&#13;
    //]]&gt;&#13;
</script>&#13;
