                    $.ajax({&#13;
                        url: "http://academic.research.microsoft.com/Images/logClick.gif?t=13&amp;query=" + query + "&amp;tm=" + new Date(),&#13;
                        success: function () {&#13;
                        }&#13;
                    });&#13;
                    $("#divLoadingIcon").show();&#13;
                    var xPosition = 0;&#13;
                    setInterval(function () {&#13;
                        if (xPosition &lt;= 0)&#13;
                            xPosition = 160;&#13;
                        xPosition = xPosition - 20;&#13;
                        $('#divLoadingIcon').css('background-position', xPosition + 'px 0px');&#13;
                    }, 125);&#13;
&#13;
                }&#13;
            );&#13;
